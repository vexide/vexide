    "cSpell.words": [
        "armv7a",
        "baudrate",
        "bitfield",
        "bitflags",
        "BITMASK",
        "centidegrees",
        "Clawbot",
        "Condvar",
        "consts",
        "errno",
        "feedforward",
        "gearset",
        "hotspot",
        "leds",
        "loopspeed",
        "opcontrol",
        "parametricity",
        "photoresistor",
        "relock",
        "repr",
        "RGBC",
        "rustc",
        "setpoint",
        "slint",
        "Symbolication",
        "symbolizer",
        "triport",
        "unconfigured",
        "ungeared",
        "varargs",
        "vcodesig",
        "vexide",
        "vexlink",
        "VEXos",
        "Vsync",
        "wirelessly",
        "Workcell",
        "zynq"
    ],
    "ltex.language": "en-US",
    "ltex.enabled": [
        "latex",
        "markdown",
        "rust"
    ],
    "rust-analyzer.cargo.allTargets": false,
    "rust-analyzer.check.targets": [
        "${workspaceFolder}/armv7a-vex-v5.json",
    ],
    "rust-analyzer.cargo.extraArgs": [
        "--examples",
        "--lib",
    ],
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.showUnlinkedFileNotification": false,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "files.eol": "\n",
}
